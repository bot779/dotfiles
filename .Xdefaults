urxvt.insecure: true
urxvt.scrollBar: false
urxvt.saveLines: 32767

urxvt.perl-ext-common: default,tabbed
urxvt.urlLauncher: /usr/local/bin/chromium
urxvt.matcher.button: 1 

urxvt.keysym.C-minus: command:\033]710;xft:Inconsolata:bold:pixelsize=14\007
urxvt.keysym.C-0: command:\033]710;xft:Inconsolata:bold:pixelsize=20\007
urxvt.keysym.C-equal: command:\033]710;xft:Inconsolata:bold:pixelsize=28\007

urxvt.font: xft:Inconsolata:bold:pixelsize=20

*depth: 32
*background: rgba:0000/0000/0000/dddd
*foreground: #e2e2e5
*termName: rxvt-256color

# Digerati
*color0:  #303030
*color1:  #c03000
*color2:  #b1d631
*color3:  #fecf35
*color4:  #426870
*color5:  #6D506D
*color6:  #4bb5c1
*color7:  #e2e2e5
*color8:  #5F5F5F
*color9:  #FF3A78
